{
	"name": "v2",
	"version": "1.0.0",
	"type": "module",
	"main": "cli.beautify.js",
	"workspaces": [
		"tools/*",
		"packages/*"
	],
	"scripts": {
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:unit": "vitest run src/tests/unit",
		"test:integration": "vitest run src/tests/integration",
		"type-check": "tsc --noEmit",
